{
    "name": "rbondoc.dev",
    "private": true,
    "version": "2.0.0",
    "type": "module",
    "scripts": {
        "build": "tsc && vite build",
        "check": "pnpm dlx @biomejs/biome check",
        "check:all": "pnpm check:types && pnpm dlx @biomejs/biome check ./src ./package.json ./postcss.config.cjs ./tailwind.config.ts ./vite.config.ts",
        "check:fix": "pnpm dlx @biomejs/biome check --apply",
        "check:fix:all": "pnpm dlx @biomejs/biome check --apply .",
        "check:format": "pnpm dlx @biomejs/biome format",
        "check:format:all": "pnpm dlx @biomejs/biome format .",
        "check:format:fix": "pnpm dlx @biomejs/biome format --write",
        "check:format:fix:all": "pnpm dlx @biomejs/biome format --write .",
        "check:lint": "pnpm dlx @biomejs/biome lint",
        "check:lint:fix": "pnpm dlx @biomejs/biome lint --apply",
        "check:lint:fix:all": "pnpm dlx @biomejs/biome lint --apply .",
        "check:lint:all": "pnpm dlx @biomejs/biome lint .",
        "check:types": "tsc --noEmit --skipLibCheck",
        "dev": "vite --host",
        "start": "vite"
    },
    "dependencies": {
        "@fortawesome/free-brands-svg-icons": "^6.4.0",
        "@fortawesome/free-regular-svg-icons": "^6.4.0",
        "@fortawesome/free-solid-svg-icons": "^6.4.0",
        "@solidjs/meta": "^0.28.6",
        "@solidjs/router": "^0.8.2",
        "lodash.throttle": "^4.1.1",
        "solid-fa": "^0.2.0",
        "solid-js": "^1.8.10",
        "solid-motionone": "^1.0.0"
    },
    "devDependencies": {
        "@biomejs/biome": "1.5.0",
        "@types/lodash.throttle": "^4.1.7",
        "@types/node": "^18.15.10",
        "autoprefixer": "^10.4.14",
        "postcss": "^8.4.27",
        "sass": "^1.64.2",
        "tailwindcss": "^3.4.1",
        "typescript": "^5.3.3",
        "typescript-plugin-css-modules": "^5.0.1",
        "vite": "^4.4.8",
        "vite-plugin-solid": "^2.7.0"
    }
}
